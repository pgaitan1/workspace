package stepdef;

import cucumber.api.java.en.Then;
import cucumber.api.java.en.When;

public class datadriventest {
	

@When("^user enters firstname \"([^\"]*)\" data driven$")
public void user_enters_firstname_data_driven(String arg1) throws Throwable {
    System.out.println(arg1);
    
}

@When("^user enters lastname \"([^\"]*)\" datadriven$")
public void user_enters_lastname_datadriven(String arg1) throws Throwable {
    System.out.println(arg1);
}

@When("^user enters mobile numbers \"([^\"]*)\" data driven$")
public void user_enters_mobile_numbers_data_driven(String arg1) throws Throwable {
    System.out.println(arg1);
}

@When("^user enters password \"([^\"]*)\" data driven$")
public void user_enters_password_data_driven(String arg1) throws Throwable {
    System.out.println(arg1);
}
@Then("^user should be able to sign up data driven$")
public void user_should_be_able_to_sign_up_data_driven() throws Throwable {


}
