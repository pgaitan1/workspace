package stepdef;

import cucumber.api.java.en.Given;
import cucumber.api.java.en.Then;
import cucumber.api.java.en.When;

public class logintest {
	
	@Given("^when the user is on the facebook homepage$")
	public void when_the_user_is_on_the_facebook_homepage() throws Throwable {
		System.out.println ("I am in the given test");
	    
	}

	@When("^user enters first name$")
	public void user_enters_first_name() throws Throwable {
		System.out.println ("I am in the when1 test");
	}

	@When("^user enters last name$")
	public void user_enters_last_name() throws Throwable {
		System.out.println ("I am in the when2 test");
	}

	@When("^user enters mobile bumber$")
	public void user_enters_mobile_bumber() throws Throwable {
		System.out.println ("I am in the when3 test");
	}

	@When("^user enters password$")
	public void user_enters_password() throws Throwable {
		System.out.println ("I am in the when4 test");
	}

	@Then("^user should be able to sign up$")
	public void user_should_be_able_to_sign_up() throws Throwable {
		System.out.println ("I am in the then test");
	}

}
