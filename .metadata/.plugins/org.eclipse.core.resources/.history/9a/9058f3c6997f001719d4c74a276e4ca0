package stepdef;

import cucumber.api.java.en.Then;
import cucumber.api.java.en.When;

public class datadriventest {
	
	@When("^user enters \"([^\"]*)\" data driven$")
	public void user_enters_data_driven(String arg1) throws Throwable {
	   System.out.println ("I am in the new when test");
	}

	@When("^user enters \"([^\"]*)\" datadriven$")
	public void user_enters_datadriven(String arg1) throws Throwable {
		System.out.println ("I am in the new when test");
	}

	@Then("^user should be able to sign up data driven$")
	public void user_should_be_able_to_sign_up_data_driven() throws Throwable {
		System.out.println ("I am in the new when test");
	}

}
